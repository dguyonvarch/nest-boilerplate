{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:int": "dotenv -e .env.test -- jest -i --no-cache --watch --config jest-int.config.json",
    "test:e2e": "dotenv -e .env.test -- jest --config ./e2e/jest-e2e.config.json",
    "dev-db:start": "docker compose up -d dev-db && sleep 1 && prisma migrate reset --force",
    "dev-db:stop": "docker compose down dev-deb",
    "dev-db:restart": "docker compose down dev-db && docker compose up -d dev-db && sleep 1",
    "test-db:restart": "docker compose down test-db && docker compose up -d test-db && sleep 1",
    "db:restart": "docker compose down && docker compose up -d && sleep 1",
    "pretest:int": "npm run test-db:restart && dotenv -e .env.test -- prisma migrate reset --force",
    "pretest:e2e": "npm run test-db:restart && dotenv -e .env.test -- prisma migrate reset --force"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.5",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.5",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.5",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/terminus": "^9.0.0",
    "@prisma/client": "^4.1.0",
    "argon2": "^0.29.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "nestjs-pino": "^3.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino-http": "^8.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^18.8.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.0.3",
    "jest-mock-extended": "^3.0.1",
    "pino-pretty": "^9.0.0",
    "prettier": "^2.5.1",
    "prisma": "^4.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
